{
    "printWidth": 100,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "semi": true,
    "plugins": [
      "prettier-plugin-sort-json",
      "@trivago/prettier-plugin-sort-imports"
    ],
    "importOrderSeparation": true,
    "importOrder": [
      "^[./]"
    ],
    "importOrderSortSpecifiers": true
}